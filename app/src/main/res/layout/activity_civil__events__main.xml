<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.jnec.techfest.swayambhu.CIVIL_EVENTS_MAIN">


    <it.moondroid.coverflow.components.ui.containers.FeatureCoverFlow

        android:id="@+id/coverflow_CIVIL"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:elevation="2dp"
        android:longClickable="true"

        app:reflectionOpacity="@android:integer/config_mediumAnimTime"
        app:coverHeight="180dp"
        app:coverWidth="120dp"
        app:maxScaleFactor="1.25"
        app:reflectionGap="1px"
        app:rotationThreshold="0.4"
        app:scalingThreshold="1.0"
        app:spacing="0.6"

        tools:ignore="OnClick">


    </it.moondroid.coverflow.components.ui.containers.FeatureCoverFlow>


    <TextSwitcher
        android:id="@+id/title_CIVIL"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:paddingTop="5dp"
        android:layout_alignParentBottom="true"
        android:layout_centerVertical="true"
        >



    </TextSwitcher>




</RelativeLayout>
